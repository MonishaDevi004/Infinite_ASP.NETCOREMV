@model IEnumerable<EKART.DTO.ProductDTO>
@{

}

<table class="table">
    <thead>
        <tr>
            <th>ProductName</th>
            <th>UnitPrice</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model )
        {
            <tr>
                <td>@item.ProductName</td>
                <td>@item.UnitPrice</td>
            </tr>
        }
    </tbody>
</table>